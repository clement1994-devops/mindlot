<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mindlot.firebase.codelab.mindlot">
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mindlot.firebase.codelab.mindlot.code.Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            />
        </activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.mains"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.tution"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.dolot"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.psc"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.contact"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.whyyou"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.whyus"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.mindlot.firebase.codelab.mindlot.code.website"
            android:label="@string/title_activity_mains"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity android:name="com.mindlot.firebase.codelab.mindlot.firebase.SignInActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <service
            android:name="com.mindlot.firebase.codelab.mindlot.firebase.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.mindlot.firebase.codelab.mindlot.firebase.MyFirebaseInstanceIdService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>
